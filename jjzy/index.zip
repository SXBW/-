        }
        @media(max-width:800px) {
            .warp {
                width:auto;
            }
            .pay-warp {
            }
        }
        @media(max-width:550px) {
            body{
                height: 100%;
                overflow: auto;
            }
            .qr-warp {
                flex-flow:column;
                width:auto;
                height:500px;
            }
            .qr-warp .qr{
                margin-bottom: 30px;
            }
        }
